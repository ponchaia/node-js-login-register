model StravaProfiles {
  id                String                            @id @default(auto()) @map("_id") @db.ObjectId
  activities        StravaprofilesActivities
  athlete           StravaprofilesAthlete
  athleteActivities StravaprofilesAthleteActivities[]
  athleteStat       StravaprofilesAthleteStat
  userId            String @unique
}

model Users {
  id                String @id @default(auto()) @map("_id") @db.ObjectId
  accessToken       String
  authorizationCode String
  email             String
  password          String
  refreshToken      String
  stravaUserId      Int
  tokenExpiresAt    Int
  tokenExpiresIn    Int
}
